  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/poseidon"
  },
  "type": "module",
  "types": "types.d.ts",
  "main": "./main.cjs",
  "module": "./src/main.js",
  "files": [
    "./dist",
